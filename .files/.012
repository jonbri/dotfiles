    t
  fi
}
2t() {
  2 $@
  count=`012 $@ --root 2 --count`
  totalCount=`012 "*" $@ --root 2 --count`
  if [ $count -eq 1 ] || [ $count -eq $totalCount ]; then
    t
  fi
}
