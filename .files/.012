0() {
  source _012 --root 0 $@
}
1() {
  source _012 --root 1 $@
}
2() {
  source _012 --root 2 $@
}
0t() {
  0 $@
  count=`012 $@ --root 0 --count`
  totalCount=`012 "*" $@ --root 0 --count`
  if [ $count -eq 1 ] || [ $count -eq $totalCount ]; then
    t
  fi
}
1t() {
  1 $@
  count=`012 $@ --root 1 --count`
  totalCount=`012 "*" $@ --root 1 --count`
