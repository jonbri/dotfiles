#!/bin/bash

0() {
  source _zeroonetwo --root 0 $@
}
1() {
  source _zeroonetwo --root 1 $@
}
2() {
  source _zeroonetwo --root 2 $@
}

0t() {
  0 $@
  count=`zeroonetwo $@ --root 0 --count`
  totalCount=`zeroonetwo "*" $@ --root 0 --count`
  if [ $count -eq 1 ] || [ $count -eq $totalCount ]; then
    t
  fi
}
1t() {
  1 $@
  count=`zeroonetwo $@ --root 1 --count`
  totalCount=`zeroonetwo "*" $@ --root 1 --count`
  if [ $count -eq 1 ] || [ $count -eq $totalCount ]; then
    t
  fi
}
2t() {
  2 $@
  count=`zeroonetwo $@ --root 2 --count`
  totalCount=`zeroonetwo "*" $@ --root 2 --count`
  if [ $count -eq 1 ] || [ $count -eq $totalCount ]; then
    t
  fi
}

