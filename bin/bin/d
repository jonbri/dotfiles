#!/bin/bash

clear
cd $DOTFILES_DIR
slow "D O T F I L E S"
echo
persona home
echo
cat << EOF
D
dot
bin
EOF
ARG=$1
if [ -z $ARG ]; then
    echo
    git email | nth 0-10
    echo
else
    if [[ "$ARG" = "bash" ]]; then
        vim $DOTFILES_DIR/.files/.bash_profile
    else
        vim $DOTFILES_DIR/bin/bin/$1
    fi
fi

